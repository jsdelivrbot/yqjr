          <result column="create_time" property="createTime"/>
          <result column="subject" property="subject"/>
          <result column="body" property="body"/>
          <result column="draft" property="draft"/>
      </collection>
      </resultMap>


      <select id="queryAllBlogInfo" resultMap="BlogInfo">
          SELECT
           B.BLOG_ID,
