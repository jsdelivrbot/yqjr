    <!--查询工作流-->
    <select id="queryWorkFlow" parameterType="com.yqjr.fin.mkmm.sales.condition.TeamWorkCondition" resultMap="TeamWorkResultMap">
        SELECT MKMM_TEAM_REVIEW.ID AS ID,
        MKMM_WORK_FLOW.ID AS WORK_ID,
        MKMM_WORK_FLOW.REVIEW_LEVEL AS REVIEW_LEVEL,
        MKMM_TEAM_REVIEW.AREA_CODE AS AREA_CODE,
        MKMM_TEAM_REVIEW.AREA_NAME AS AREA_NAME,
        MKMM_TEAM_REVIEW.TEAM_CODE AS TEAM_CODE,
        MKMM_TEAM_REVIEW.TEAM_NAME AS TEAM_NAME,
        MKMM_TEAM_REVIEW.TEAM_LEVEL AS TEAM_LEVEL,
        MKMM_TEAM_REVIEW.TEAM_TYPE AS TEAM_TYPE,
        MKMM_TEAM_REVIEW.TEAM_STATUS AS TEAM_STATUS,
        MKMM_TEAM_REVIEW.TEAM_LEADER AS TEAM_LEADER,
        MKMM_TEAM_REVIEW.TEAM_PHONE AS TEAM_PHONE,
        MKMM_TEAM_REVIEW.TEAM_ZIP AS TEAM_ZIP,
        MKMM_TEAM_REVIEW.TEAM_ADDRESS AS TEAM_ADDRESS,
        MKMM_TEAM_REVIEW.TEAM_FAX AS TEAM_FAX,
        MKMM_TEAM_REVIEW.START_TIME AS START_TIME,
        MKMM_TEAM_REVIEW.CREATE_TIME AS CREATE_TIME,
        MKMM_TEAM_REVIEW.MODIFY_TIME AS MODIFY_TIME,
        MKMM_TEAM_REVIEW.CREATOR AS CREATOR,
        MKMM_TEAM_REVIEW.MODIFIER AS MODIFIER,
        MKMM_TEAM_REVIEW.REMARK AS REMARK,
        MKMM_TEAM_REVIEW.REVIEW_STATUS AS REVIEW_STATUS,
        MKMM_TEAM_REVIEW.REVIEW_TYPE AS REVIEW_TYPE,
        MKMM_TEAM_REVIEW.FLAG AS FLAG,
        MKMM_TEAM_REVIEW.TRANSCODE1 AS TRANSCODE1,
        MKMM_TEAM_REVIEW.TRANSCODE2 AS TRANSCODE2,
        MKMM_TEAM_REVIEW.TRANSCODE3 AS TRANSCODE3,
        MKMM_TEAM_REVIEW.TRANSCODE4 AS TRANSCODE4,
        MKMM_TEAM_REVIEW.CREATE_NAME AS CREATE_NAME,
        MKMM_TEAM_REVIEW.COMPANY AS COMPANY,
        MKMM_TEAM_REVIEW.FOUNDER_LEVEL AS FOUNDER_LEVEL
        FROM MKMM_WORK_FLOW ,
        (SELECT REVIEW_ID,MAX(ID) AS MAX_ID FROM MKMM_WORK_FLOW GROUP BY REVIEW_ID
        ) MAX_WORK_FLOW ,
        MKMM_TEAM_REVIEW
        WHERE MKMM_WORK_FLOW.REVIEW_ID  =MAX_WORK_FLOW.REVIEW_ID
        AND MKMM_WORK_FLOW.ID           =MAX_WORK_FLOW.MAX_ID
        AND MKMM_WORK_FLOW.REVIEW_ID    =MKMM_TEAM_REVIEW.TEAM_CODE
        AND MKMM_TEAM_REVIEW.FLAG       ='1'
        AND MKMM_WORK_FLOW.REVIEW_STATUS in('02','03')
        <if test="company!=null and company!=''">
            AND MKMM_TEAM_REVIEW.COMPANY = #{company,jdbcType=VARCHAR}<!--公司代码-->
        </if>
        <if test="areaCode!=null and areaCode!=''">
            AND MKMM_TEAM_REVIEW.AREA_CODE LIKE CONCAT(#{areaCode},'%')<!--区域代码-->
        </if>
        <if test="areaName!=null and areaName!=''">
            AND MKMM_TEAM_REVIEW.AREA_NAME LIKE CONCAT(#{areaName},'%')<!--区域名称-->
        </if>
        <if test="teamCode!=null and teamCode!=''">
            AND MKMM_TEAM_REVIEW.TEAM_CODE LIKE CONCAT(#{teamCode},'%')<!--团队代码-->
        </if>
        <if test="teamName!=null and teamName!=''">
            AND MKMM_TEAM_REVIEW.TEAM_NAME LIKE CONCAT(#{teamName},'%')<!--团队名称-->
        </if>
        <if test="modType!=null and modType!=''">
            AND MKMM_WORK_FLOW.MOD_TYPE     = #{modType,jdbcType=VARCHAR}
        </if>
        <if test="reviewLevel!=null and reviewLevel!=''">
            AND MKMM_WORK_FLOW.REVIEW_LEVEL =#{reviewLevel,jdbcType=VARCHAR}
        </if>
        <if test="reviewType!=null and reviewType!=''">
