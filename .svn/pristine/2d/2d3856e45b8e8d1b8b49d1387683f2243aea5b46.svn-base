        </if>
        <if test="founderLevel != null">
            <choose>
                <when test="founderLevel == 03">AND MKMM_TEAM_REVIEW.FOUNDER_LEVEL in ('02','03')</when>
                <when test="founderLevel == 02">AND MKMM_TEAM_REVIEW.FOUNDER_LEVEL in ('01')</when>
            </choose>
        </if>
    </select>
