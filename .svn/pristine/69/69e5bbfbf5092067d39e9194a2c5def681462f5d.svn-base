package com.yqjr.fin.risk.stat.utils;

/**
 * 常量
 * @author cuiyan
 * @since  2018/9/6
 */
public class Constant {
    /** 定时任务状态 */
    public enum ScheduleStatus {
        /** 正常*/
    	NORMAL(0),
        /**  暂停*/
    	PAUSE(1);

        private int value;

        ScheduleStatus(int value) {
            this.value = value;
        }
        
        public int getValue() {
            return value;
        }
    }
}
