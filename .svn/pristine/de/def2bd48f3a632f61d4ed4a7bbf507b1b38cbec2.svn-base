    <!--查询审核通过列表-->
    <select id="selectPersonMainTable" parameterType="com.yqjr.fin.mkmm.sales.condition.PersonWorkCondition"
            resultMap="personMainTableResultMap">
        select
        <include refid="Base_Person_Main_Table"/>
        from MKMM_TEAM_MAIN A,
        MKMM_PERSON_TEAM B,
        MKMM_PERSON_MAIN C
        WHERE
        A.TEAM_CODE = B.TEAM_CODE
        AND B.PERSON_CODE = C.PERSON_CODE
        <if test="personCode != null and personCode!=''">
            AND C.PERSON_CODE LIKE CONCAT(#{personCode},'%')
        </if>
        <if test="personName != null and personName!=''">
            AND C.PERSON_NAME LIKE CONCAT(#{personName},'%')
        </if>
        <!--<if test="personNature !=selectPersonMainTable null and personNature!=''">-->
        <!--AND B.PERSON_NATURE = #{personNature}-->
        <!--</if>-->
        <if test="reviewType != null and reviewType!=''">
            AND B.REVIEW_TYPE LIKE CONCAT(#{reviewType},'%')
        </if>
        <if test="reviewStatus != null and reviewStatus!=''">
            AND B.REVIEW_STATUS LIKE CONCAT(#{reviewStatus},'%')
        </if>
        <if test="personNature != null and personNature!=''">
            AND B.PERSON_NATURE = #{personNature}
        </if>
        <if test="personStatus != null and personStatus!=''">
            AND B.PERSON_STATUS = #{personStatus}
        </if>
        <if test="areaCode!= null and areaCode!=''">
            AND B.AREA_CODE = #{areaCode}
        </if>
        <if test="company!= null and company!=''">
            AND B.COMPANY = #{company}
        </if>
        <if test="teamCode!= null and teamCode!=''">
            AND B.TEAM_CODE = #{teamCode}
        </if>
        ORDER BY B.PERSON_CODE DESC
    </select>

    <!--通过id查询销售人员信息-->
    <select id="selectPersonMainById" parameterType="java.lang.Long" resultMap="personMainDetailResultMap">
        select
        <include refid="Base_Person_Main"/>
        from MKMM_TEAM_MAIN A,
        MKMM_PERSON_TEAM B,
        MKMM_PERSON_MAIN C,
        MKMM_PERSON_CITY D,
        MKMM_PERSON_BRAND E
        WHERE A.TEAM_CODE = B.TEAM_CODE
        AND B.PERSON_CODE = C.PERSON_CODE
        AND C.PERSON_CODE = D.PERSON_CODE(+)
        AND C.PERSON_CODE = E.PERSON_CODE(+)
        <if test="_parameter != null">
            AND C.ID = #{_parameter}
        </if>
    </select>


    <!--通过flowId查询审核通过人员信息-->
    <select id="selectPersonMainByflowId" parameterType="java.lang.Long" resultMap="personMainDetailResultMap">
        select
        <include refid="Base_Person_Main"/>
        from MKMM_TEAM_MAIN A,
        MKMM_PERSON_TEAM_REVIEW B,
        MKMM_PERSON_MAIN C,
        MKMM_PERSON_CITY D,
        MKMM_PERSON_BRAND E

        WHERE A.TEAM_CODE = B.TEAM_CODE
        AND B.PERSON_CODE = C.PERSON_CODE
        AND C.PERSON_CODE = D.PERSON_CODE(+)
        AND C.PERSON_CODE = E.PERSON_CODE(+)


        <if test="_parameter != null">
            AND B.FLOW_ID = #{_parameter}

        </if>
    </select>

