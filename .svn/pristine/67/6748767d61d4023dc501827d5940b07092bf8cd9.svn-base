           B.AUTHOR_ID AS BLOG_AUTHOR_ID,
           A.AUTHOR_ID,
           A.USER_NAME,
           A.PASSWORD,
           A.EMAIL,
           A.BIOGRAPHY,
           P.POST_ID,
           P.BLOG_ID AS BLOG_POST_ID ,
             P.CREATE_TIME,
           P.SUBJECT,
           P.BODY,
           P.DRAFT
      FROM BLOG B
      LEFT OUTER JOIN AUTHOR A
        ON B.AUTHOR_ID = A.AUTHOR_ID
      LEFT OUTER JOIN POST P
