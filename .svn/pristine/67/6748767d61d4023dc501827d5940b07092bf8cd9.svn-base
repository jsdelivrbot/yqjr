        select
        <include refid="Active_Review_List" />
        from MKMM_ACTIVE_MAIN_REVIEW A
        LEFT JOIN MKMM_WORK_FLOW B
        ON A.ACTIVE_CODE = B.REVIEW_ID,
        (SELECT REVIEW_ID,MAX(ID) AS MAX_ID FROM MKMM_WORK_FLOW GROUP BY REVIEW_ID) C
        <where>
            A.FLAG = '1'
            AND B.ID=C.MAX_ID
            AND A.REVIEW_STATUS in('02','03')
            <if test="company != null">
                AND A.COMPANY = #{company,jdbcType=VARCHAR}
            </if>
            <if test="activeCode != null">
                AND A.ACTIVE_CODE LIKE CONCAT(#{activeCode,jdbcType=VARCHAR},'%')
            </if>
            <if test="activeName != null">
                AND A.ACTIVE_NAME LIKE CONCAT(#{activeName,jdbcType=VARCHAR},'%')
            </if>
            <if test="reviewType != null">
                AND A.REVIEW_TYPE = #{reviewType,jdbcType=VARCHAR}
            </if>
            <if test="reviewStatus != null">
                AND A.REVIEW_STATUS = #{reviewStatus,jdbcType=VARCHAR}
            </if>
            <if test="reviewLevel != null">
                AND B.REVIEW_LEVEL = #{reviewLevel,jdbcType=VARCHAR}
            </if>
            <if test="areaCode != null">
                AND A.AREA_CODE = #{areaCode,jdbcType=VARCHAR}
            </if>
            <if test="founderLevel != null">
                <choose>
                    <when test="founderLevel == 03">AND A.FOUNDER_LEVEL in ('02','03')</when>
                    <when test="founderLevel == 02">AND A.FOUNDER_LEVEL in ('01')</when>
                </choose>
            </if>
        </where>
        ORDER BY A.ID
    </select>

