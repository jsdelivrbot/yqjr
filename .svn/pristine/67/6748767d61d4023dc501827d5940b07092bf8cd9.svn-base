            <if test="areaCode != null">
                AND A.AREA_CODE = #{areaCode,jdbcType=VARCHAR}
            </if>
            <if test="brandsCode != null">
                AND E.BRANDS_CODE = #{brandsCode,jdbcType=VARCHAR}
            </if>
        </where>
        ORDER BY A.ID
    </select>

    <update id="updateFlagById" parameterType="java.lang.Long">
        update <include refid="Base_Table_Name" />
        SET FLAG = '0'
        where ID = #{mid,jdbcType=NUMERIC}
    </update>

    <update id="updateStatusById">
        update <include refid="Base_Table_Name" />
        set REVIEW_STATUS = #{status,jdbcType=VARCHAR},
