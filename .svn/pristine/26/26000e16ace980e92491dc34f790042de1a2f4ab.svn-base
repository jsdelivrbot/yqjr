    <!-- 计算计划跟踪 -->
    <select id="sumPlanTrack" resultMap="TrackVoBaseResultMap" parameterType="com.yqjr.fin.mkmm.sales.model.PlanTrackVo">
        select aa.area_name as area_name,aa.person_name as person_name, aa.person_type as person_type,SUM(aa.month_plan_num) as month_plan_num, SUM(aa.month_actual_num) as month_actual_num,aa.MONTH_RATE,SUM(bb.YEAR_PLAN_NUM) AS YEAR_PLAN_NUM ,SUM(bb.year_actual_num) as year_actual_num, bb.YEAR_RATE
        from mkmm_plan_track aa, mkmm_plan_track_year bb
        where aa.person_code = bb.person_code
        and aa.year = bb.year
        and aa.brands = bb.brands
        and aa.company = bb.company
        and aa.year = #{year}
        and aa.month = #{month}
        <if test="areaCode != null and areaCode!=''">
            and aa.area_code = #{areaCode}
        </if>
        <if test="brands != null and brands!=''">
            and aa.brands = #{brands}
        </if>
        <if test="personName != null and personName!=''">
            AND aa.PERSON_NAME LIKE CONCAT(#{personName},'%')
        </if>
        and aa.company = #{company}
        group by aa.area_name ,aa.person_name, aa.person_type, aa.MONTH_RATE, bb.YEAR_RATE
    </select>

