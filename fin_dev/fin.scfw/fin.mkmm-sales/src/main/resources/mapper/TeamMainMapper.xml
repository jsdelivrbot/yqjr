        END as TEAM_STATUS,
        TEAM_LEADER,TEAM_PHONE,TEAM_ZIP,TEAM_ADDRESS,TEAM_FAX,START_TIME,CREATE_TIME,MODIFY_TIME,REMARK,TRANSCODE1,TRANSCODE2,TRANSCODE3,TRANSCODE4,CREATOR,MODIFIER,REVIEW_ID,COMPANY
        from <include refid="Base_Table_Name" />
        where ID = #{id,jdbcType=INTEGER}
    </select>

