    <select id="selectByLoginName" parameterType="java.lang.String" resultMap="BaseResultMap">
            select a.*,c.code as areaCode,c.name as areaName from sys_user a
            join  sys_user_zone b  on b.user_id=a.id  
            right join  sys_zone c   on b.zone_id=c.id
            where a.login_name=#{loginName}
            and a.status=1
    </select>
    
    <select id="findUserLoginByName" parameterType="java.lang.String" resultMap="BaseResultMap1">
            select a.*,c.code as areaCode,c.name as areaName from sys_user a
            join  sys_user_zone b  on b.user_id=a.id  
            right join  sys_zone c   on b.zone_id=c.id
            where a.login_name=#{loginName}
            <if test="areaCode !='R0000' ">
             and  c.code =#{areaCode}
             </if>
             and  a.comCode =#{comCode}
             and a.status=1
    </select>

    <select id="getUserByName" parameterType="java.lang.String" resultMap="BaseResultMap1">
            select a.*,c.code as areaCode,c.name as areaName from sys_user a
            join  sys_user_zone b  on b.user_id=a.id  
            right join  sys_zone c   on b.zone_id=c.id
              WHERE a.USER_NAME = '%${userName}%'
